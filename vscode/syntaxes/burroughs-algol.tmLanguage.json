{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Burroughs ALGOL",
	"scopeName": "source.algol.burroughs",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#preprocessor"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#functions"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.block.algol",
					"begin": "(?i)\\bCOMMENT\\b",
					"end": ";",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.begin.algol"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.comment.end.algol"
						}
					}
				},
				{
					"name": "comment.line.percentage.algol",
					"match": "%.*$"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.algol",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.algol",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.algol",
					"match": "\\b[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?\\b"
				}
			]
		},
		"preprocessor": {
			"name": "keyword.control.preprocessor.algol",
			"match": "\\$(?i:STRINGS|INCLUDE|CHECK|CODE|LEVEL|LINEINFO|LIST|SET|POP|PAGE|SHARING|SINGLE|STACK|STATISTICS|TADS|VERSION)\\b"
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.algol",
					"match": "(?i)\\b(BEGIN|END|IF|THEN|ELSE|FOR|DO|WHILE|UNTIL|STEP|GO|CASE|CONTINUE)\\b"
				},
				{
					"name": "storage.type.algol",
					"match": "(?i)\\b(INTEGER|REAL|BOOLEAN|ARRAY|POINTER|PROCEDURE|DOUBLE|LONG|ALPHA|FILE|FORMAT|LABEL|LIST|OWN|REFERENCE|SWITCH|TASK|TRANSLATETABLE|TRUTHSET|VALUE|EVENT|EXCEPTION|DIRECT|EPILOG|PROLOG|ZIP)\\b"
				},
				{
					"name": "constant.language.algol",
					"match": "(?i)\\b(TRUE|FALSE|NULL)\\b"
				},
				{
					"name": "keyword.operator.logical.algol",
					"match": "(?i)\\b(AND|OR|NOT|EQV|IMP|DIV|MOD|EQL|NEQ|GTR|LSS|GEQ|LEQ|IS|ISNT)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.assignment.algol",
					"match": ":="
				},
				{
					"name": "keyword.operator.comparison.algol",
					"match": "(==|!=|<|>|<=|>=)"
				},
				{
					"name": "keyword.operator.arithmetic.algol",
					"match": "(\\+|\\-|\\*|\\/|\\^)"
				},
				{
					"name": "punctuation.definition.bitfield.algol",
					"match": "\\[\\d+:\\d+\\]"
				}
			]
		},
		"functions": {
			"name": "support.function.algol",
			"match": "(?i)\\b(ABORT|ABORTTRANSACTION|ABS|ACCEPT|ACCEPTCLOSE|ACCEPTOPEN|ACTUALNAME|AFTER|ALL|ALPHA7|ALPHA8|ANY|ANYFAULT|ANYTYPE|APPLYINSERT|APPLYMODIFY|ARCCOS|ARCSIN|ARCTAN|ARCTAN2|ARRAYS|ARRAYSEARCH|AS|ASCII|ASCIITOEBCDIC|ASCIITOHEX|ASSIGN|ASSOCIATEDDATA|ASSOCIATAEDDATALENGTH|ATANH|ATEND|ATTACH|ATTRIBINFORMER|ATTRIBSEARCHER|AVAILABLE|AWAITOPEN|BACKUPPREFIX|BASE|BEFORE|BEGINTRANSACTION|BINARY|BLOCK|BUILDATTLIST|BY|BYFUNCTION|BYTITLE|CABS|CALL|CALLING|CANCEL|CANCELTRPOINT|CAT|CAUSE|CAUSEANDRESET|CCOS|CEXP|CHANGEFILE|CHARGECODE|CLASS|CHECKPOINT|CHECKSUM|CLN|CLOSE|CLOSEDISPOSITION|CODE|COMBINEPPBS|COMPILETIME|COMPILETYPE|COMPLEX|COMSRECORD|CONJUGATE|CONNECTTIMELIMIT|CONNECTION|CONTROL|COREESTIMATE|COS|COSH|COTAN|CREATE|CRUNCH|CSIN|CSQRT|CURRENT|DABS|DALPHA|DAND|DARCCOS|DARCSIN|DARCTAN|DARCTAN2|DATABASE|DBS|DCOS|DCOSH|DCSYSTEMTABLES|DEALLOCATE|DECIMAL|DECIMALPOINTISCOMMA|DECLAREDPRIORITY|DEFINE|DELINKLIBRARY|DELTA|DEQV|DERF|DERFC|DETACH|DEXP|DGAMMA|DICTIONARY|DIGITS|DIMP|DINTEGER|DINTEGERT|DISABLE|DISCARD|DISK|DISKPACK|DISPLAY|DLGAMMA|DLN|DLOG|DMABS|DMADD_DAYS|DMADD_TIME|DMAVG|DMAX|DMCHR|DMCONTAINS|DMCOUNT|DMDAY|DMDAY_OF_WEEK|DMELAPSED_DAYS|DMELAPSED_TIME|DMEQUIV|DMEXCEPTIONINFO|DMEXCEPTIONMSG|DMEXCLUDES|DMEXITS|DMEXT|DMHOUR|DMIN|DMISA|DMLENGTH|DMMATCH|DMMAX|DMMIN|DMMINUTE|DMMONTH|DMMONTHNAME|DMNEXTEXCEPTION|DMPOS|DMPRED|DMRECORD|DMROUND|DMRPT|DMSQRT|DMSUCC|DMSUM|DMTERMINATE|DMTEST|DMTRUNC|DMYEAR|DNABS|DNORMALIZE|DNOT|DONTWAIT|DONTWAITFORFILE|DOR|DREAL|DROP|DSCALELEFT|DSCALERIGHT|DSCALERIGHTT|DSIN|DSINH|DSQRT|DTAN|DTANH|DUMP|EBCDIC|EBCDICTOASCII|EBCDICTOHEX|EGI|ELAPSEDTIME|EMI|EMPTY|EMPTY4|EMPTY7|EMPTY8|ENABLE|ENDTRANSACTION|ENTIER|ENTITY|ERF|ERFC|ESI|EXCEPTIONEVENT|EXCEPTIONTASK|EXCHANGE|EXCLUDE|EXISTS|EXP|EXPONENTOVERFLOW|EXPONENTUNDERFLOW|EXPORT|EXPORTLIBRARY|EXTERNAL|FAMILY|FILECARDS|FILES|FILL|FIND|FIRST|FIRSTONE|FIRSTWORD|FIX|FORMAL|FORMRECORDLIBRARY|FORWARD|FUNCTIONNAME|FREE|FREEZE|FROM|GAMMA|GET|HAPPENED|HEAD|HEX|HEXTOASCII|HEXTOEBCDIC|HISTORY|IMAG|IMPORTED|IN|INCLUDE|INITIATOR|INPUTHEADER|INSERT|INSERT_CLIENTQUEUE|INTEGEROVERFLOW|INTEGERT|INTERLOCK|INTERRUPT|INTERRUPTIBLE|INTNAME|INTERFACENAME|INTRINSICINFO|INVALIDADDRESS|INVALIDINDEX|INVALIDOP|INVALIDPROGRAMWORD|INVERSE|ISVALID|JOBNUMBER|LAST|LB|LENGTH|LIBACCESS|LIBERATE|LIBPARAMETER|LIBRARIES|LIBRARY|LINE|LINENUMBER|LINKLIB_CLIENTQUEUE|LINKLIBRARY|LISTLOOKUP|LN|LNGAMMA|LOCK|LOCKED|LOCKOWNER|LOCKSTATUS|LOG|LOOP|MASKSEARCH|MAX|MAXIOTIME|MAXLINES|MAXPROCTIME|MEMORYPARITY|MEMORYPROTECT|MERGE|MESSAGECOUNT|MESSAGESEARCHER|MIDTRANSACTION|MIDTRANSLATE|MIN|MLSACCEPT|MLSDISPLAY|MODIFY|MONITOR|MUX|MYJOB|MYSELF|NABS|NAME|NO|NOCR|NOLF|NONE|NORMALIZE|NUMBER|NUMERIC|OF|OFFER|OFFSET|ON|ONES|OPEN|OPTION|ORDER|ORDERLY|ORGUNIT|OUT|OUTPUTHEADER|OUTPUTMESSAGE|PACK|PAGE|PAGED|PARTICIPATE|PARTNER|PENDING|PERMANENT|PICTURE|POTC|POTH|POTL|PRIVATE|PRIVATELIBRARIES|PROCESS|PROCESSID|PROCESSIOTIME|PROCESSTIME|PROCURE|PRODUCTION|PROGRAMDUMP|PROGRAMMEDOPERATOR|PUBLIC|PURGE|PUT|QUERY|RANDOM|RB|READ|READFORM|READLOCK|READONLY|READWRITE|READYCL|RECEIVE|RECORD|RECREATE|REEL|REJECTCLOSE|REJECTOPEN|REMAININGCHARS|REMOVE|REMOVEFILE|REPEAT|REPLACE|RESET|RESIDENT|RESIZE|RESPOND|RESPONDTYPE|RESTART|RETAIN|RETRIEVE|RETRIEVE_WARNING|REWIND|RUN|SAVETRPOINT|SCALELEFT|SCALERIGHT|SCALERIGHTF|SCALERIGHTT|SCAN|SCANPARITY|SDIGITS|SECONDWORD|SECURE|SEEK|SELECT|SEMANTIC|SEND|SET|SETABSTRACTVALUE|SETACTUALNAME|SETTOCHILD|SETTOPARENT|SIBS|SIGN|SIN|SINGLE|SINH|SIZE|SKIP|SOME|SORT|SPACE|SQRT|STACKER|STACKNO|STACKSIZE|STARTINSERT|STARTMODIFY|STARTTIME|STATION|STATUS|STOP|STOPPOINT|STORE|STRING|STRINGPROTECT|STRING4|STRING7|STRING8|STRUCTURE|STRUCTURENUMBER|SUBFILE|SUBROLE|SYNCHRONIZE|TADS|TAIL|TAKE|TAN|TANH|TARGETTIME|TASKATTERR|TASKFILE|TASKVALUE|TERMINAL|TEST|THIS|THISCL|THRU|TIME|TIMELIMIT|TIMES|TITLE|TO|TRANSACTION|TRANSITIVE|TRANSLATE|TRY|TYPE|UNITS|UNLOCK|UNLOCKED|UNPACKED|UNREADYCL|URGENT|USERCODE|USERDATA|USERDATALOCATOR|USERDATAREBUILD|USING|WAIT|WAITANDRESET|WAITFORFILE|WHEN|WHERE|WITH|WORDS|WRITE|WRITEFORM|ZERODIVIDE)\\b"
		}
	}
}
